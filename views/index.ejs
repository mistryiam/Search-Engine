    <!-- Optional JavaScript; choose one of the two! -->
    <script>
       const form = document.querySelector("form");
       const questionElement = form.question;
       const titles = document.querySelectorAll(".title");
       const urls = document.querySelectorAll(".url");
       const quesBody = document.querySelectorAll(".body");
       const loadingDiv = document.querySelector(".loading");
      //var x=document.getElementById("doss").value();
      form.addEventListener("submit", async (e) => {
        e.preventDefault();
        const question = questionElement.value;
         console.log(question);
          loadingDiv.innerHTML = `Loading...`;
         try {
      const res = await fetch(`/search?question=${question}`, {
        method: "GET",
      });
      
      const data = await res.json();
      loadingDiv.innerHTML = ``;
      for (let i = 0; i < 5; i++) {
        titles[i].innerHTML = `<h1>${data[i].title}</h1>`;
      }
      for (let i = 0; i < 5; i++) {
        urls[i].innerHTML = `Link: <h3>${data[i].url}</h3>`;
      }
      for (let i = 0; i < 5; i++) {
        quesBody[i].innerHTML = `<h5>${data[i].body}</h5>`;
      }
    } catch (error) {
      alert(error);
    }
  });

    </script>
